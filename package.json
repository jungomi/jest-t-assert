{
  "name": "jest-t-assert",
  "version": "0.1.0",
  "description": "Use tape style assertions with Jest",
  "main": "lib/index.js",
  "module": "src/index.js",
  "author": "Michael Jungo <michaeljungo92@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jungomi/jest-t-assert.git"
  },
  "files": [
    "lib",
    "src"
  ],
  "keywords": [
    "assert",
    "assertion",
    "jest",
